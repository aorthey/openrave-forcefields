        <extents>1.0 1.0 0.1</extents>
        <translation>5.0 -3.0 -0.1</translation>
        <diffusecolor>0.6 0.6 0.6</diffusecolor>
      </geom>
    </body>
  </kinbody>


  <physicsengine type="ode">
    <odeproperties>
      <friction>0.0</friction>
      <gravity>0 0 -9.8</gravity>
      <selfcollision>1</selfcollision>
    </odeproperties>
