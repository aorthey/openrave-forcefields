    <body name="upper_floor" type="static">
      <geom type="box">
        <extents>4.0 1.0 0.1</extents>
        <translation>-2.0 1.0 -0.1</translation>
        <diffusecolor>0.6 0.6 0.6</diffusecolor>
      </geom>
    </body>
    <body name="middle_floor" type="static">
      <geom type="box">
        <extents>1.0 1.0 0.1</extents>
